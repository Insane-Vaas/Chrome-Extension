{
    "manifest_version": 2,
    "name": "My Extension",
    "version": "0.1.0",
    "content_scripts":[
        {
            "matches":["*://*.youtube.com/*"],
            "js":["main.js"]
        }  
    ],
    "browser_action": {
        "default_title": "Bear"
      },
      "background": {
        "scripts": ["background.js"]
      },
      "permissions": [
        "notifications",
        "tabs"
      ]
}