{
    "manifest_version": 3,
    "name": "My Extension",
    "version": "0.1.0",
    "content_scripts":[
        {
            "matches":["*://*.youtube.com/*"],
            "js":["main.js"]
        }  
    ],   
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
      "notifications",
      "tabs"
    ]
}